<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="cursoJpaAduana2019_EJB" transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>cl.aduana.entities.Categoria</class>
		<class>cl.aduana.entities.Detalle</class>
		<class>cl.aduana.entities.Producto</class>
		<class>cl.aduana.entities.Usuario</class>
		<class>cl.aduana.entities.Venta</class>
		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.Oracle10gDialect" />
			<property name="javax.persistence.jdbc.user" value="curso"/>
			<property name="javax.persistence.jdbc.password" value="curso"/>
			<property name="javax.persistence.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
		</properties>
	</persistence-unit>
</persistence>
